[program:psdash-master]
command=PSDASH_CONFIG="/etc/psdash.py" {{ psdash.path }}/bin/psdash -p 7000 -l /var/log/*.log -l /var/log/**/*.log
;command={{ psdash.path }}/bin/psdash -l /var/log/**/*.log -l /var/log/*.log
;directory="{{ psdash.path }}"
environment=PSDASH_CONFIG="/etc/psdash.py",PATH="{{ psdash.path }}/bin:{{ lookup('env', 'PATH') }}"
